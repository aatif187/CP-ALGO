//Function to return list containing elements of right view of binary tree.
    vector<int> rightView(Node *root)
    {
       // Your Code here
       vector<int>ans;
        queue<Node*>q;
        if(!root) return ans;
        q.push(root);
        while(!q.empty()){
            int sz=q.size();
            ans.push_back(q.front()->data);
            while(sz--){
            auto tp= q.front();
            q.pop();
            
            
            if(tp->right)q.push(tp->right);
            if(tp->left)q.push(tp->left);
        }
        }
         return ans;
    }
