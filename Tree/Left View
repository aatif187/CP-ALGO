//Function to return a list containing elements of left view of the binary tree.
vector<int> leftView(Node *root)
{
   // Your code here
   vector<int>ans;
        queue<Node*>q;
        if(!root) return ans;
        q.push(root);
        while(!q.empty()){
            int sz=q.size();
            ans.push_back(q.front()->data);
            while(sz--){
            auto tp= q.front();
            q.pop();
            
            if(tp->left)q.push(tp->left);
            if(tp->right)q.push(tp->right);
        }
        }
        return ans;
}
 
